    <xs:complexType name="Grupo_RRC0005_ArrajPgto_ComplexType">
        <xs:sequence>
            <xs:element name="CodInstitdrArrajPgto" type="CodInstitdrArrajPgtoErr"/>
        </xs:sequence>
        <xs:attribute name='CodErro' type='CodErro'/>
